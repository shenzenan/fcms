<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
        package="org.bitholic.dao">

    <class name="RouteStation" table="routeStation">
        <id name="id" column="ID">
        </id>
        <property name="stationID" column="stationID" not-null="true" />
        <property name="routeName" column="routeName" not-null="true" />
        <property name="sequence" column="sequence" not-null="true" />
    </class>
</hibernate-mapping>
