<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
        package="org.bitholic.dao">

    <class name="Shift" table="shift">
        <id name="shiftID" column="shiftID">
        </id>
        <property name="routeName" column="routeName" not-null="true" />
        <property name="startTime" column="startTime" not-null="true" />
        <property name="licensePlate" column="licensePlate" />
        <property name="driverID" column="driverID" />
    </class>
</hibernate-mapping>
